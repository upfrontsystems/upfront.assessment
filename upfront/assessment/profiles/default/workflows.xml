<?xml version="1.0"?>
<object name="portal_workflow" meta_type="Plone Workflow Tool">

 <property
    name="title">Contains workflow definitions for upfront.assessment</property>

 <object name="evaluation_workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="upfront.assessment.content.evaluation">
   <bound-workflow workflow_id="evaluation_workflow" />
  </type>
 </bindings>

 <object name="evaluationsheet_workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="upfront.assessment.content.evaluationsheet">
   <bound-workflow workflow_id="evaluationsheet_workflow" />
  </type>
 </bindings>

 <object name="assessment_workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="upfront.assessment.content.assessment">
   <bound-workflow workflow_id="assessment_workflow" />
  </type>
 </bindings>

 <object name="classlist_workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="upfront.classlist.content.classlist">
   <bound-workflow workflow_id="classlist_workflow" />
  </type>
 </bindings>

</object>
